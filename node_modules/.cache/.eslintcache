\n            {results?.map(({ links, id, source, title }, i) => {\n              return (\n                <div key={i} className='w-full md:w-2/5'>\n                  <a\n                    href={links?.[0].href}\n                    target='_blank'\n                    rel='noreferrer'\n                    className='hover:underline'\n                  >\n                    <p className='text-lg text-blue-700 dark:text-blue-300'>\n                      {title}\n                    </p>\n                  </a>\n                  <div className='flex gap-4'>\n                    <a href={source?.href} target='_blank' rel='noreferrer'>\n                      {source?.href}\n                    </a>\n                  </div>\n                </div>\n              );\n            })}\n          </div>\n        );\n        break;\n      case'/image':\n        return (\n          <div className='flex flex-wrap justify-center'>\n            {results?.map(({ image: { src }, link: { href, title } }, i) => (\n              <div key={i} className='w-screen p-2 lg:w-1/4 md:w-1/2 sm:w-full'>\n                <a\n                  href={href}\n                  target='_blank'\n                  rel='noreferrer'\n                  className='flex flex-col justify-between w-full h-full p-5 border border-gray-400 rounded shadow sm:p-3 dark:border-gray-200'\n                >\n                  <img\n                    src={src}\n                    alt={title}\n                    loading='lazy'\n                    className='w-full max-h-40'\n                  />\n                  <p className='mt-2 break-words text-small'>{title}</p>\n                </a>\n              </div>\n            ))}\n          </div>\n        );\n        break;\n      case'/video':\n        return (\n          <div className='flex flex-wrap justify-center'>\n            {results?.map((video, i) => {\n              if (\n                !(\n                  video.additional_links?.[0].href.includes('channel') ||\n                  video.additional_links?.[0].href.includes('playlist')\n                )\n              ) {\n                return (\n                  <div key={i} className='p-2'>\n                    <ReactPlayer\n                      url={video.additional_links?.[0].href}\n                      controls\n                      width='100%'\n                      height='100%'\n                    />\n                  </div>\n                );\n              }\n            })}\n          </div>\n        );\n        break;\n      default:\n        return'ERROR!';\n    }\n  }\n};\n\nexport default Results;\n","/Users/vanessamitelman/NODEJS PROJECT/React API Projects/voogl/src/components/Loading.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/voogl/src/components/Links.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/voogl/src/components/NotFound.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/index.js",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/App.js",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/contexts/ResultContextProvider.js",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Footer.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Routes.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Navbar.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Search.jsx",["124"],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Results.jsx",["125","126","127","128","129","130"],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/NotFound.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/components/Links.jsx",[],[],"/Users/vanessamitelman/NODEJS PROJECT/React API Projects/Voogl/src/c